{
  "name": "clubs",
  "version": "0.11.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:prettier:astro",
    "lint:eslint": "eslint --fix '**/*.{js,mjs,cjs}'",
    "lint:prettier": "prettier -w --ignore-path .astro.prettierignore --no-error-on-unmatched-pattern .",
    "lint:prettier:astro": "yarn prettier -w **/*.astro || echo 'ðŸ™ˆ > ignored some errors of prettier for astro'",
    "prepare": "husky install && tsc -p ./packages/cli",
    "buildCli": "tsc -p ./packages/cli"
  },
  "bin": {
    "clubs": "./packages/cli/lib/index.js"
  },
  "devDependencies": {
    "@types/dotenv": "8.2.0",
    "@types/ramda": "0.28.14",
    "dotenv": "16.0.1",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.2.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "husky": "8.0.1",
    "postcss": "8.4.14",
    "prettier": "2.6.2",
    "prettier-plugin-astro": "0.1.0-next.5",
    "prettier-plugin-tailwindcss": "0.1.11",
    "remark-cli": "10.0.1",
    "sass": "1.52.3",
    "type-fest": "2.13.0",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/node": "^17.0.42",
    "@types/yargs": "^17.0.10",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "astro": "1.0.0-beta.42",
    "bignumber.js": "9.0.2",
    "ethers": "5.6.8",
    "ramda": "0.28.0",
    "rxjs": "7.5.5",
    "vue": "3.2.37",
    "chalk": "4.1.2",
    "debug": "^4.3.4",
    "findup": "^0.1.5",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.4",
    "path": "^0.12.7",
    "yargs": "^17.5.1"
  },
